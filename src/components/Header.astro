<header class=" shadow-gray-700 max-w-full h-[70px] py-2 bg-[#010739] flex flex-row items-center overflow-hidden 2xl:max-w-full pb-2">
    <nav class=" w-full p-5 shadow md:flex justify-between">
        <div class="flex justify-between items-center"> 
            <span class="text-white text-xl mx-2 md:hidden block">
                <ion-icon name="menu" id="menu-hamburguer" class="w-[35px] h-[35px] transition ease-in duration-500"></ion-icon>
            </span>
            <img class="w-20 h-20" src="data:image/jpeg;base64," alt="">


            
        </div>
        <ul class=" md:flex md:items-center z-[-1] md:z-auto md:static absolute bg-[#010739] w-full left-0 md:w-auto md:py-0 py-4 md:pl-0 pl-3 md:opacity-100 opacity-0  transition-all ease-in duration-500">
            <li class="mx-4 my-6  md:my-0"><a href="/" class="text-lg font-bold hover:border-b text-white pb-2  duration-500">Inicio</a></li>
            <li class="mx-4 my-6 md:my-0"><a href="#cartelera" class="text-lg font-bold hover:border-b text-white pb-2 duration-500">Cartelera</a></li>
            <li class="mx-4 my-6 md:my-0"><a href="#" class="text-lg font-bold hover:border-b text-white pb-2  duration-500">Estrenos</a></li>
            <li class="mx-4 my-6 md:my-0"><a href="#" class="text-lg font-bold hover:border-b text-white pb-2   duration-500">Contactos</a></li>
            <li class="mx-4 my-6 md:my-0"><a href="/login" class="text-lg font-bold hover:border-b text-white pb-2  duration-500">Ingresar</a></li>
        
        </ul>
    </nav>
</header>
   

<script>
const menuHamburger = document.querySelector('#menu-hamburguer')
menuHamburger?.addEventListener('click',Menu)
function Menu(){
    let attr = menuHamburger?.getAttribute('name')
    let list = document.querySelector('ul')

    if(attr === 'menu'){
        menuHamburger?.removeAttribute('menu')
        menuHamburger?.setAttribute('name','close')
        list?.classList.add('top-[70px]'),
        list?.classList.add('z-[100]'),
        list?.classList.add('opacity-100')
    }else{
        menuHamburger?.removeAttribute('close')
        menuHamburger?.setAttribute('name','menu')
        list?.classList.remove('top-[30px]'),
        list?.classList.remove('z-[100]'),
        list?.classList.remove('opacity-100')
    }
}




</script>